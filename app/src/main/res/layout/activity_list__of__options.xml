<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".List_Of_Options">

    <Button
        android:id="@+id/statusBTn"
        android:layout_width="293dp"
        android:layout_height="57dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="59dp"
        android:text="Status"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contactBtn" />

    <Button
        android:id="@+id/callogBtn"
        android:layout_width="291dp"
        android:layout_height="59dp"
        android:layout_marginStart="61dp"
        android:layout_marginTop="71dp"
        android:layout_marginEnd="59dp"
        android:text="Call Logs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/contactBtn"
        android:layout_width="286dp"
        android:layout_height="60dp"
        android:layout_marginStart="61dp"
        android:layout_marginTop="69dp"
        android:layout_marginEnd="64dp"
        android:text="Contacts"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/callogBtn" />

    <Button
        android:id="@+id/getSms"
        android:layout_width="293dp"
        android:layout_height="53dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="58dp"
        android:text="Messages"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statusBTn" />
</androidx.constraintlayout.widget.ConstraintLayout>